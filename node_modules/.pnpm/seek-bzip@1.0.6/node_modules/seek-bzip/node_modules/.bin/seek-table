#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/runner/work/act/act/node_modules/.pnpm/seek-bzip@1.0.6/node_modules/seek-bzip/bin/node_modules:/home/runner/work/act/act/node_modules/.pnpm/seek-bzip@1.0.6/node_modules/seek-bzip/node_modules:/home/runner/work/act/act/node_modules/.pnpm/seek-bzip@1.0.6/node_modules:/home/runner/work/act/act/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/runner/work/act/act/node_modules/.pnpm/seek-bzip@1.0.6/node_modules/seek-bzip/bin/node_modules:/home/runner/work/act/act/node_modules/.pnpm/seek-bzip@1.0.6/node_modules/seek-bzip/node_modules:/home/runner/work/act/act/node_modules/.pnpm/seek-bzip@1.0.6/node_modules:/home/runner/work/act/act/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/seek-bzip-table" "$@"
else
  exec node  "$basedir/../../bin/seek-bzip-table" "$@"
fi
