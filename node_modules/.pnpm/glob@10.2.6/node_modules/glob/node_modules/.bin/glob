#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/runner/work/act/act/node_modules/.pnpm/glob@10.2.6/node_modules/glob/dist/cjs/src/node_modules:/home/runner/work/act/act/node_modules/.pnpm/glob@10.2.6/node_modules/glob/dist/cjs/node_modules:/home/runner/work/act/act/node_modules/.pnpm/glob@10.2.6/node_modules/glob/dist/node_modules:/home/runner/work/act/act/node_modules/.pnpm/glob@10.2.6/node_modules/glob/node_modules:/home/runner/work/act/act/node_modules/.pnpm/glob@10.2.6/node_modules:/home/runner/work/act/act/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/runner/work/act/act/node_modules/.pnpm/glob@10.2.6/node_modules/glob/dist/cjs/src/node_modules:/home/runner/work/act/act/node_modules/.pnpm/glob@10.2.6/node_modules/glob/dist/cjs/node_modules:/home/runner/work/act/act/node_modules/.pnpm/glob@10.2.6/node_modules/glob/dist/node_modules:/home/runner/work/act/act/node_modules/.pnpm/glob@10.2.6/node_modules/glob/node_modules:/home/runner/work/act/act/node_modules/.pnpm/glob@10.2.6/node_modules:/home/runner/work/act/act/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/cjs/src/bin.js" "$@"
else
  exec node  "$basedir/../../dist/cjs/src/bin.js" "$@"
fi
