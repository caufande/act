#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/runner/work/act/act/node_modules/.pnpm/@ohm-js+cli@2.0.0_ohm-js@17.1.0/node_modules/@ohm-js/cli/node_modules:/home/runner/work/act/act/node_modules/.pnpm/@ohm-js+cli@2.0.0_ohm-js@17.1.0/node_modules/@ohm-js/node_modules:/home/runner/work/act/act/node_modules/.pnpm/@ohm-js+cli@2.0.0_ohm-js@17.1.0/node_modules:/home/runner/work/act/act/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/runner/work/act/act/node_modules/.pnpm/@ohm-js+cli@2.0.0_ohm-js@17.1.0/node_modules/@ohm-js/cli/node_modules:/home/runner/work/act/act/node_modules/.pnpm/@ohm-js+cli@2.0.0_ohm-js@17.1.0/node_modules/@ohm-js/node_modules:/home/runner/work/act/act/node_modules/.pnpm/@ohm-js+cli@2.0.0_ohm-js@17.1.0/node_modules:/home/runner/work/act/act/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../index.js" "$@"
else
  exec node  "$basedir/../../index.js" "$@"
fi
