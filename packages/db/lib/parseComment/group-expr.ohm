GroupExpr {
	Expr
		= ExprAnd
		| ExprOr
		| Term

	ExprAnd
		= Term (and Term)+

	ExprOr
		= Term (or Term )+

	Term
		= groupName -- name
		| not Term -- not
		| Quoted<Expr> -- expr

	Quoted<T> = "(" T ")"

	groupName = (~ (and | or | not | space | "(" | ")") any)+
	not = "!"
	and = "&"
	or = "|"
}
