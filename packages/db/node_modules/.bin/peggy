#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/runner/work/act/act/node_modules/.pnpm/peggy@4.2.0/node_modules/peggy/bin/node_modules:/home/runner/work/act/act/node_modules/.pnpm/peggy@4.2.0/node_modules/peggy/node_modules:/home/runner/work/act/act/node_modules/.pnpm/peggy@4.2.0/node_modules:/home/runner/work/act/act/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/runner/work/act/act/node_modules/.pnpm/peggy@4.2.0/node_modules/peggy/bin/node_modules:/home/runner/work/act/act/node_modules/.pnpm/peggy@4.2.0/node_modules/peggy/node_modules:/home/runner/work/act/act/node_modules/.pnpm/peggy@4.2.0/node_modules:/home/runner/work/act/act/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../peggy/bin/peggy.js" "$@"
else
  exec node  "$basedir/../peggy/bin/peggy.js" "$@"
fi
